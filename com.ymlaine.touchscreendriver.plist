<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.ymlaine.touchscreendriver</string>
    <key>ProgramArguments</key>
    <array>
        <string>/usr/local/bin/TouchscreenDriver</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <dict>
        <key>SuccessfulExit</key>
        <false/>
    </dict>
    <key>StandardOutPath</key>
    <string>/tmp/touchscreendriver.log</string>
    <key>StandardErrorPath</key>
    <string>/tmp/touchscreendriver.log</string>
</dict>
</plist>
